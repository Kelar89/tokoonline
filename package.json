{
  "name": "toko-whatsapp-eleventy",
  "version": "1.0.0",
  "description": "Website toko online berbasis WhatsApp menggunakan Eleventy dan Bootstrap 5.",
  "main": ".eleventy.js",
"scripts": {
    "sass:build": "sass assets/_scss/main.scss assets/css/style.css",
    "sass:watch": "sass --watch assets/_scss/main.scss assets/css/style.css",
    "eleventy:build": "eleventy",
    "eleventy:serve": "eleventy --serve",
    "start": "concurrently \"eleventy --serve\" \"sass --watch assets/_scss/main.scss assets/css/style.css\"",
    "build": "npm-run-all sass:build eleventy:build"
  },
  "keywords": [
    "eleventy",
    "jamstack",
    "bootstrap5",
    "toko-online",
    "whatsapp"
  ],
  "author": "Admin Toko",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.6"
  }
}